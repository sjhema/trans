package com.primovision.lutransport.model;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name = "fuel_toll_netvalues")
public class FuelAndTollNetValues extends AbstractBaseModel{
	
 
  protected Long refid;
  
  @Column(name="net_fule_value")
  protected Double fuelNetValue;
  
  @Column(name="net_toll_value")
  protected Double tollNetValue;

public Long getRefid() {
	return refid;
}

public void setRefid(Long refid) {
	this.refid = refid;
}

public Double getFuelNetValue() {
	return fuelNetValue;
}

public void setFuelNetValue(Double fuelNetValue) {
	this.fuelNetValue = fuelNetValue;
}

public Double getTollNetValue() {
	return tollNetValue;
}

public void setTollNetValue(Double tollNetValue) {
	this.tollNetValue = tollNetValue;
}
  
  
  
  
	

}
